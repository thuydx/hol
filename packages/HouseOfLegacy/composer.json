{
    "name": "thuydx/house-of-legacy",
    "description": "The package to define the static data, mapping and relations for the House of Legacy.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ThuyDX",
            "email": "me@thuydx.pro"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "ThuyDX\\HouseOfLegacy\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThuyDX\\HouseOfLegacy\\Providers\\HouseOfLegacyServiceProvider"
            ],
            "aliases": {
                "HouseOfLegacy": "ThuyDX\\HouseOfLegacy\\Facades\\HouseOfLegacy"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
