{
    "name": "thuydx/sessiondb",
    "description": "A fake database for Laravel using session or JSON storage.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ThuyDX",
            "email": "me@thuydx.pro"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "ThuyDX\\SessionDb\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThuyDX\\SessionDb\\Providers\\SessionDbServiceProvider"
            ],
            "aliases": {
                "SessionDb": "ThuyDX\\SessionDb\\Facades\\SessionDb"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
